<h1>Ironhack Cinema</h1>
<h2>Click on the movie to check the showtimes</h2>

<div class="filmContainter">
  {{#each movies}}
  <div class="film">
    <div class="img"><img src="{{this.image}}" alt="film"> </div>
    <div class="title">{{this.title}}</div>
    <div class="seeMore"><a href="/movie/{{this.id}}">see more</a></div>
  </div>
  {{/each}}
</div>